+      x1r = a(0) - a(4)
+      x1i = a(1) - a(5)
+      x2r = a(2) + a(6)
+      x2i = a(3) + a(7)
+      x3r = a(2) - a(6)
+      x3i = a(3) - a(7)
+      a(0) = x0r + x2r
+      a(1) = x0i + x2i
+      a(2) = x1r - x3i
+      a(3) = x1i + x3r
+      a(4) = x0r - x2r
+      a(5) = x0i - x2i
+      a(6) = x1r + x3i
+      a(7) = x1i - x3r
+      end
+!
+      subroutine cftb040(a)
+      real*8 a(0 : 7), x0r, x0i, x1r, x1i, x2r, x2i, x3r, x3i
+      x0r = a(0) + a(4)
+      x0i = a(1) + a(5)
+      x1r = a(0) - a(4)
+      x1i = a(1) - a(5)
+      x2r = a(2) + a(6)
+      x2i = a(3) + a(7)
+      x3r = a(2) - a(6)
+      x3i = a(3) - a(7)
+      a(0) = x0r + x2r
+      a(1) = x0i + x2i
+      a(2) = x1r + x3i
+      a(3) = x1i - x3r
+      a(4) = x0r - x2r
+      a(5) = x0i - x2i
+      a(6) = x1r - x3i
+      a(7) = x1i + x3r
+      end
+!
+      subroutine cftx020(a)
+      real*8 a(0 : 3), x0r, x0i
+      x0r = a(0) - a(2)
+      x0i = a(1) - a(3)
+      a(0) = a(0) + a(2)
+      a(1) = a(1) + a(3)
+      a(2) = x0r
+      a(3) = x0i
+      end
+!
+      subroutine rftfsub(n, a, nc, c)
+      integer n, nc, j, k, kk, ks, m
+      real*8 a(0 : n - 1), c(0 : nc - 1), wkr, wki, xr, xi, yr, yi
+      m = n / 2
+      ks = 2 * nc / m
+      kk = 0
+      do j = 2, m - 2, 2
+          k = n - j
+          kk = kk + ks
+          wkr = 0.5d0 - c(nc - kk)
+          wki = c(kk)
+          xr = a(j) - a(k)
+          xi = a(j + 1) + a(k + 1)
+          yr = wkr * xr - wki * xi
+          yi = wkr * xi + wki * xr
+          a(j) = a(j) - yr
+          a(j + 1) = a(j + 1) - yi
+          a(k) = a(k) + yr
+          a(k + 1) = a(k + 1) - yi
+      end do
+      end
+!
+      subroutine rftbsub(n, a, nc, c)
+      integer n, nc, j, k, kk, ks, m
+      real*8 a(0 : n - 1), c(0 : nc - 1), wkr, wki, xr, xi, yr, yi
+      m = n / 2
+      ks = 2 * nc / m
+      kk = 0
+      do j = 2, m - 2, 2
+          k = n - j
+          kk = kk + ks
+          wkr = 0.5d0 - c(nc - kk)
+          wki = c(kk)
+          xr = a(j) - a(k)
+          xi = a(j + 1) + a(k + 1)
+          yr = wkr * xr + wki * xi
+          yi = wkr * xi - wki * xr
+          a(j) = a(j) - yr
+          a(j + 1) = a(j + 1) - yi
+          a(k) = a(k) + yr
+          a(k + 1) = a(k + 1) - yi
+      end do
+      end
+!
+      subroutine dctsub(n, a, nc, c)
+      integer n, nc, j, k, kk, ks, m
+      real*8 a(0 : n - 1), c(0 : nc - 1), wkr, wki, xr
+      m = n / 2
+      ks = nc / n
+      kk = 0
+      do j = 1, m - 1
+          k = n - j
+          kk = kk + ks
+          wkr = c(kk) - c(nc - kk)
+          wki = c(kk) + c(nc - kk)
+          xr = wki * a(j) - wkr * a(k)
+          a(j) = wkr * a(j) + wki * a(k)
+          a(k) = xr
+      end do
+      a(m) = c(0) * a(m)
+      end
+!
+      subroutine dstsub(n, a, nc, c)
+      integer n, nc, j, k, kk, ks, m
+      real*8 a(0 : n - 1), c(0 : nc - 1), wkr, wki, xr
+      m = n / 2
+      ks = nc / n
+      kk = 0
+      do j = 1, m - 1
+          k = n - j
+          kk = kk + ks
+          wkr = c(kk) - c(nc - kk)
+          wki = c(kk) + c(nc - kk)
+          xr = wki * a(k) - wkr * a(j)
+          a(k) = wkr * a(k) + wki * a(j)
+          a(j) = xr
+      end do
+      a(m) = c(0) * a(m)
+      end
+!
