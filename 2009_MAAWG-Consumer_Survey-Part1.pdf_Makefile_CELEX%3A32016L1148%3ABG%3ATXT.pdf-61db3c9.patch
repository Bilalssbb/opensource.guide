+#include <stdio.h>
+#include <stdlib.h>
+#define fft3d_alloc_error_check(p) { \
+    if ((p) == NULL) { \
+        fprintf(stderr, "fft3d memory allocation error\n"); \
+        exit(1); \
+    } \
+}
