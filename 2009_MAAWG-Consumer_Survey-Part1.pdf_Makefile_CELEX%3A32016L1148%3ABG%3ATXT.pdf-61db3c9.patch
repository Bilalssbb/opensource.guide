+      err = errorcheck3d(nmax, nmax, n1, n2, n3, 
+     &    8.0d0 / n1 / n2 / n3, a)
+      write (*, *) 'ddct3d err= ', err
+!
+!   check of DDST
+      call putdata3d(nmax, nmax, n1, n2, n3, a)
