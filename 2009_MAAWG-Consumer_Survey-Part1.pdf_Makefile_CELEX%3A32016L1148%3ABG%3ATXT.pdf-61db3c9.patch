+      integer n, nc, j, k, kk, ks, m
+      real*8 a(0 : n - 1), c(0 : nc - 1), wkr, wki, xr
+      m = n / 2
+      ks = nc / n
+      kk = 0
+      do j = 1, m - 1
+          k = n - j
+          kk = kk + ks
+          wkr = c(kk) - c(nc - kk)
+          wki = c(kk) + c(nc - kk)
+          xr = wki * a(j) - wkr * a(k)
+          a(j) = wkr * a(j) + wki * a(k)
+          a(k) = xr
+      end do
+      a(m) = c(0) * a(m)
+      end
+!
+      subroutine dstsub(n, a, nc, c)
+      integer n, nc, j, k, kk, ks, m
+      real*8 a(0 : n - 1), c(0 : nc - 1), wkr, wki, xr
+      m = n / 2
+      ks = nc / n
+      kk = 0
+      do j = 1, m - 1
+          k = n - j
+          kk = kk + ks
+          wkr = c(kk) - c(nc - kk)
+          wki = c(kk) + c(nc - kk)
+          xr = wki * a(k) - wkr * a(j)
+          a(k) = wkr * a(k) + wki * a(j)
+          a(j) = xr
+      end do
+      a(m) = c(0) * a(m)
+      end
+!
