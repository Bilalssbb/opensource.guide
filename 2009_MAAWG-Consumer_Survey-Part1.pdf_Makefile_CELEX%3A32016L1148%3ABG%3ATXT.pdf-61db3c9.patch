+
+void putdata2d(int n1, int n2, double **a)
+{
+    int j1, j2, seed = 0;
+
+    for (j1 = 0; j1 <= n1 - 1; j1++) {
+        for (j2 = 0; j2 <= n2 - 1; j2++) {
+            a[j1][j2] = RND(&seed);
+        }
+    }
+}
