+                  x3r = a(j2, i) - a(j3, i)
+                  x3i = a(j2 + 1, i) - a(j3 + 1, i)
+                  a(j, i) = x0r + x2r
+                  a(j + 1, i) = x0i + x2i
+                  a(j2, i) = x0r - x2r
+                  a(j2 + 1, i) = x0i - x2i
+                  a(j1, i) = x1r + x3i
+                  a(j1 + 1, i) = x1i - x3r
+                  a(j3, i) = x1r - x3i
+                  a(j3 + 1, i) = x1i + x3r
+              end do
+              if (m .lt. n) then
+                  wk1r = w(2)
+                  do j = m, l + m - 2, 2
+                      j1 = j + l
+                      j2 = j1 + l
+                      j3 = j2 + l
+                      x0r = a(j, i) + a(j1, i)
+                      x0i = a(j + 1, i) + a(j1 + 1, i)
+                      x1r = a(j, i) - a(j1, i)
+                      x1i = a(j + 1, i) - a(j1 + 1, i)
