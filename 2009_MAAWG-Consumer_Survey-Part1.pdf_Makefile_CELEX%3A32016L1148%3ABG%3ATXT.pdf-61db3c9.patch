+                        a[j][i + 1] = x0i + x2i;
+                        x0r -= x2r;
+                        x0i -= x2i;
+                        a[j2][i] = wk2r * x0r + wk2i * x0i;
+                        a[j2][i + 1] = wk2r * x0i - wk2i * x0r;
+                        x0r = x1r + x3i;
+                        x0i = x1i - x3r;
+                        a[j1][i] = wk1r * x0r + wk1i * x0i;
+                        a[j1][i + 1] = wk1r * x0i - wk1i * x0r;
+                        x0r = x1r - x3i;
+                        x0i = x1i + x3r;
