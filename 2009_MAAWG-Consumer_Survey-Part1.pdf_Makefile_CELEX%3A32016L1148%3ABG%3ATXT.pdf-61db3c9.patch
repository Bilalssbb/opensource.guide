+      do i = 0, n2 - 1
+          kk = 0
+          do k = n / 2 - 2, 2, -2
+              j = n - k
+              kk = kk + ks
+              wkr = 0.5d0 - c(kk)
+              wki = c(nc - kk)
+              xr = a(k, i) - a(j, i)
+              xi = a(k + 1, i) + a(j + 1, i)
+              yr = wkr * xr - wki * xi
+              yi = wkr * xi + wki * xr
+              a(k, i) = a(k, i) - yr
+              a(k + 1, i) = a(k + 1, i) - yi
