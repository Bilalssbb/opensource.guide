+double errorcheck2d(int n1, int n2, double scale, double **a)
+{
+    int j1, j2, seed = 0;
+    double err = 0, e;
+
+    for (j1 = 0; j1 <= n1 - 1; j1++) {
+        for (j2 = 0; j2 <= n2 - 1; j2++) {
+            e = RND(&seed) - a[j1][j2] * scale;
